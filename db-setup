CREATE ROLE app_user WITH LOGIN PASSWORD 'your_password';


-- Other useful comments

with user_data AS (
	SELECT id FROM users WHERE username = 'johku'
)
INSERT INTO uploads (
	user_id,
	upload_time,
	song_name,
	song_description,
	filepath,
	filename
	)
	VALUES (
	(SELECT id from user_data),
	NOW()::TIMESTAMP,
	'testi',
	'oaod',
	'data/',
	'test.txt'
);